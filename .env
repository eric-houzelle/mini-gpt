DATASET_NAME=wexhi/text-to-svg
# DATASET_KEY is unused when DATASET_TEMPLATE is provided but kept for reference
DATASET_KEY=svg
TOKENIZER_NAME=camembert-base
MODEL_SAVE_PATH=checkpoints/best_miniGPT.pt
DATASET_TEMPLATE="Prompt: {caption}\n\n<SVG>\n{svg}\n</SVG>"
PROMPT_TEMPLATE="PROMPT_START\n{prompt}\nPROMPT_END\nSVG_START\n"
STOP_SEQUENCE="SVG_END"
EVAL_PROMPT="Caption: minimalist sunrise over layered mountains\nInstructions: Use polygons for peaks, a gradient sky, and a rising sun."